
IF	//1st Neera Romance Throne of Bhaal Rest. EXTEND_BOTTOM ~neera25.bcs~
	Global("OHN_TOB_PLOT","GLOBAL",4)
	AreaCheck("oh6400")
	OR(2)
		TimeLT(3)
		TimeGT(15)
	Global("YF_restcheck","GLOBAL",0)
THEN
	RESPONSE #100
	SetGlobal("YF_restcheck","GLOBAL",1)
END

IF
	Global("OHN_TOB_PLOT","GLOBAL",5)
	Global("YF_OHN_TOB_PLOT5","GLOBAL",0)
THEN
	RESPONSE #100
	SetGlobal("YF_restcheck","GLOBAL",2)
	SetGlobal("YF_OHN_TOB_PLOT5","GLOBAL",1)
END


